<!DOCTYPE html>
<html lang="en">

<head>
        <title>
                <%=title%>
        </title>
        <script src="/javascripts/fen_test.js"></script>
</head>

<body>
        <div class="table-data">
                <% if(userData.length!=0){ var i=1; userData.forEach(function(data){ %>
                        <form name="form" id="pp_<%= data.test_id%>">
                                <table border="1">
                                        <tr>
                                                <th>
                                                        Soru <%=i;%>
                                                </th>
                                        </tr>
                                        <tr>
                                                <td>
                                                        <img src="<%= data.question %>" alt="<%= data.name %>">
                                                </td>
                                        </tr>
                                        <tr>
                                                <th>
                                                        <input type="radio" id="answer_a_<%=data.test_id%>"
                                                                name="answer" value="a">
                                                        <label for="answer_a">A</label>
                                                        <input type="radio" id="answer_b_<%=data.test_id%>"
                                                                name="answer" value="b">
                                                        <label for="answer_b">B</label>
                                                        <input type="radio" id="answer_c_<%=data.test_id%>"
                                                                name="answer" value="c">
                                                        <label for="answer_c">C</label>
                                                        <input type="radio" id="answer_d_<%=data.test_id%>"
                                                                name="answer" value="d">
                                                        <label for="answer_d">D</label>
                                                </th>
                                        </tr>
                                </table>
                        </form>


                        <% i++; }) } else{ %>
                                <tr>
                                        <td colspan="7">No Data Found</td>
                                </tr>
                                <% } %>
                                        <submit type='button' onclick="submitForm('<%= JSON.stringify(userData) %>')"
                                                value='Click to Submit Both Forms'>
                                                kaydet
                                        </submit>

        </div>

</body>

</html>